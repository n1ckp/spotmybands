{
  "name": "spotmybands",
  "version": "0.1.0",
  "engines": {
    "node": "14.15.4"
  },
  "repository": "https://github.com/n1ckp/spotmybands",
  "description": "The place for quickly spotting events for the bands you listen to",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "NODE_ENV=production webpack --config ./src/client/webpack.config.js",
    "start:client-dev": "webpack-dev-server --config ./src/client/webpack.config.js --host 0.0.0.0",
    "build:server": "tsc --p ./src/server",
    "start": "node ./built/server/server.js",
    "start:server-dev": "ts-node --project ./src/server/tsconfig.json ./src/server/server",
    "copy:assets": "node ./scripts/copyAssets.js",
    "heroku-postbuild": "npm install && npm run build:server && npm run build:client && npm run copy:assets",
    "storybook": "start-storybook",
    "heroku:setkeys": "node ./scripts/setHerokuKeys.js"
  },
  "keywords": [
    "Spotify",
    "Songkick",
    "Google",
    "Maps",
    "Bands",
    "Music",
    "Live",
    "Music",
    "Gigs"
  ],
  "author": "Nick Phillips",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@storybook/addon-knobs": "^6.1.17",
    "@storybook/react": "^6.1.17",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.3",
    "css-loader": "^2.1.1",
    "date-fns": "^1.30.1",
    "eslint": "^5.15.1",
    "eslint-plugin-react": "^7.12.4",
    "isomorphic-fetch": "^3.0.0",
    "node-sass": "^4.11.0",
    "pigeon-maps": "^0.17.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-svg-loader": "^3.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.25",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "hbs": "^4.1.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
